{
  "name": "fwf",
  "version": "1.0.0",
  "description": "F W Frost Engineers website built using Eleventy3",
  "main": "eleventy.config.js",
  "author": "Will moody",
  "license": "MIT",
  "type": "module",
  "browserslist": [
    "last 2 versions",
    "> 0.5%"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"npm run clean\" \"npm run eleventy:dev\" \"npm run watch:sass\" ",
    "build": "concurrently \"npm run clean\" \"npm run favicons\" \"npm run eleventy:prod\" \"npm run sass:prod\" && npx pagefind --site _site --force-language unknown",
    "eleventy:dev": "cross-env ELEVENTY_ENV=development npx @11ty/eleventy --serve --quiet",
    "watch:sass": "npx sass ./src/assets/scss:_site/assets/css --watch",
    "sass:prod": "npx sass --no-source-map --style=compressed ./src/assets/scss:_site/assets/css",
    "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
    "clean": "del-cli _site",
    "favicons": "node ./src/config/setup/generate-favicons.js",
    "prettier": "concurrently \"node ./prettier.config.js\" && prettier --write '**/**/*.{js,md,scss,css}'"
  },
  "dependencies": {
    "@11ty/eleventy": "3.1.0-alpha.1",
    "@11ty/eleventy-img": "^v6.0.2",
    "@11ty/eleventy-plugin-rss": "^2.0.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@11ty/font-awesome": "^1.0.0-beta.1",
    "@awesome.me/kit-c1b42413a1": "^1.0.4"
  },
  "devDependencies": {
    "@my-ginger-pussy/eleventy-social-images": "^1.0.2",
    "@toycode/markdown-it-class": "^1.2.4",
    "@uncenter/eleventy-plugin-toc": "^1.0.3",
    "autoprefixer": "^10.4.20",
    "bulma": "^0.9.4",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "esbuild": "^0.23.1",
    "html-minifier-terser": "^7.2.0",
    "lodash": "^4.17.21",
    "lodash.replace": "^4.1.4",
    "luxon": "^3.5.0",
    "markdown-it": "^14.1.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-prism": "^2.3.0",
    "markdown-it-table-of-contents": "^0.8.0",
    "netlify-plugin-cache": "^1.0.3",
    "pagefind": "^1.2.0",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-jinja-template": "^1.5.0",
    "sass": "1.77.6",
    "sharp": "^0.33.5",
    "sharp-ico": "^0.1.5",
    "slugify": "^1.6.6"
  }
}
